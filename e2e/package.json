	"scripts": {
		"single-test": "testcafe chrome --assertion-timeout 30000 --page-load-timeout 30000 --skip-js-errors --debug-on-fail --test",
		"full-tests": "node cli-scripts/full-tests",
		"testcafe": "testcafe",
		"test": "echo \"Error: no test specified\" && exit 1",
		"testCafe_chrome": "testcafe chrome tests/",
		"testCafe_ie": "testcafe IE tests/",
		"testCafe_firefox": "testcafe firefox tests/",
		"testCafe_CI_Execute_Tests": "node cli-scripts/execute-tests",
		"testCafe_CI_Execute_Smoke_Tests": "node cli-scripts/execute-smoke-tests",
		"testcafe_smoke": "testcafe -c 6 chrome:headless tests/ --test-meta TestRun=Smoke -e --page-load-timeout 60000 --assertion-timeout 60000 --selector-timeout 60000",
		"format": "eslint **/*.ts --fix && eslint **/*.js --fix"
	},
